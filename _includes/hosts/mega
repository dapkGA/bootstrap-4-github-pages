<script>
function getQueryVariable(e){for(var r=window.location.search.substring(1),t=r.split("&"),n=0;n<t.length;n++){var a=t[n].split("=");if(a[0]==e)return a[1]}return!1}window.onload=function(){var klik=f=getQueryVariable("f"),s=getQueryVariable("s"),e=getQueryVariable("hash"),x="https://mega.nz/#!";document.getElementById("download").innerHTML=f+" "+s.class=" btn btn-success",document.getElementById("download").href=x+e;document.getElementById("notice").innerHTML="ready to download".class="alert alert--success};</script>
